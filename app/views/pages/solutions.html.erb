/* Horizontal Dropdown Menu Styling */
.fix-nav.horizontal-dropdown {
  display: flex;
  justify-content: center;
  list-style: none; /* Remove bullet points */
}

.fix-nav.horizontal-dropdown .nav-item {
  position: relative;

}

.fix-nav.horizontal-dropdown .dropdown-toggle {
  padding: 8px 15px;
  text-decoration: none; /* Remove underlines */
}





.fix-nav.horizontal-dropdown .dropdown-menu {
  display: none;
  position: absolute;
  top: 100%;
  left: 0;
  background-color: white;
  border: none;
  /* box-shadow: 0 2px 5px rgba(0, 0, 0, 0.2); */
  z-index: 1000;
  min-width: 100%;
  text-align: left;
  list-style: none; /* Remove bullet points from dropdown */
  padding: 0; /* Remove default padding from dropdown list */
}

.fix-nav.horizontal-dropdown .dropdown-item {
  padding: 8px 15px;
  color: #226F54;
  display: block;
  clear: both;
  white-space: nowrap;
  font-size: 13px; 
  text-decoration: none; /* Remove underlines from dropdown items */
  font-family: 'Figtree', sans-serif;
}

.fix-nav.horizontal-dropdown .nav-item:hover .dropdown-menu,
.fix-nav.horizontal-dropdown .nav-item .dropdown-menu:hover {
  display: block;
}

/* Remove line from first word next to logo */
